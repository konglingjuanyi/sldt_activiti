<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sldt.mds.dmc.mp.vo.MyDeclareVO"
		table="T_MP_DECLARE_INFO" lazy="true" batch-size="20">
		<id name="decId" type="java.lang.String">
			<column name="DEC_ID" length="32" />
			<generator class="assigned" />
		</id>
		<property name="decName" type="java.lang.String">
			<column name="DEC_NAME" length="128" />
		</property>
		<property name="decDesc" type="java.lang.String">
			<column name="DEC_DESC" length="1024" />
		</property>
		<property name="altOperDate" type="java.lang.String">
			<column name="ALT_OPER_DATE" length="50" />
		</property>
		<property name="altUser" type="java.lang.String">
			<column name="ALT_USER" length="32" />
		</property>
		<property name="actId" type="java.lang.String">
			<column name="ACT_ID" length="64" />
		</property>
		<property name="actSts" type="java.lang.String">
			<column name="ACT_STS" length="1" />
		</property>
		<property name="lastModifier" type="java.lang.String">
			<column name="LAST_MODIFIER" length="32" />
		</property>
		<property name="lastModifyTime" type="java.lang.String">
			<column name="LAST_MODIFY_TIME" length="50" />
		</property>
		<property name="decType" type="java.lang.String">
			<column name="DEC_TYPE" length="1" />
		</property>
	</class>
</hibernate-mapping>